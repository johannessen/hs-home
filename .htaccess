<IfModule mod_autoindex.c>
	IndexOptions TrackModified ScanHTMLTitles DescriptionWidth=* FancyIndexing IconWidth=32 IconHeight=32 IgnoreCase -IconsAreLinks NameWidth=* -SuppressDescription -SuppressHTMLPreamble Charset=windows-1252
	<IfModule mod_version.c>
		<IfVersion >= 2.2>
			<IfModule mod_autoindex.c>
				IndexOptions +VersionSort
			</IfModule>
		</IfVersion>
	</IfModule>
	HeaderName HEADER.html
	ReadmeName README.html
	IndexIgnore images
	AddIcon (DIR,/~joar0011/images/GenericFolder.png) ^^DIRECTORY^^
	AddIcon /icons/blank.gif ^^BLANKICON^^
	AddIcon /~joar0011/images/back.gif ..
	AddIconByType (PDF,/~joar0011/images/pdf.png) application/pdf
	AddIcon /~joar0011/images/java.png .java
	AddIcon /~joar0011/images/class.png .class
	AddIcon /~joar0011/images/jar.png .jar
	AddIconByType (HTM,/~joar0011/images/html.png) text/html
	AddIconByType (CSS,/~joar0011/images/css.png) text/css
	AddIcon /~joar0011/images/GenericCode.png .c .cpp .cc .m .r .h .js .pl .p .rb .php
	AddIcon /~joar0011/images/GenericCompressed.png .zip .sit .tgz .gz .bin .hqx .tar .dmg .sitx
	AddIconByType (IMG,/~joar0011/images/GenericImage.png) image/*
	AddIconByType (SND,/~joar0011/images/GenericAudio.png) audio/*
	AddIconByType (VID,/~joar0011/images/GenericVideo.png) video/*
	AddIconByType (TXT,/~joar0011/images/txt.png) text/*
	DefaultIcon /~joar0011/images/GenericDocument.png
</IfModule>



<IfModule mod_mime.c>
	AddType "application/octet-stream" dae
	<Files *.txt>
		ForceType "text/plain; charset=utf-8"
	</Files>
	<Files *.java>
		ForceType "text/x-java; charset=utf-8"
	</Files>
</IfModule>
